{
"source": {
"include": ["app", "components"],
"exclude": ["node_modules", ".next", "out", "docs", "app/globals.css"]
},
"plugins": ["plugins/markdown"],
"opts": {
"destination": "docs",
"recurse": true,
"readme": "README.md",
"template": "node_modules/clean-jsdoc-theme",
"encoding": "utf8"
},
"templates": {
"cleverLinks": true,
"monospaceLinks": true
}
}